{
  "name": "dockerps",
  "version": "0.3.0",
  "description": "Pretty-print docker ps utility",
  "main": "./lib/index.js",
  "bin": {
    "dockerps": "bin/dockerps.js"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "watch 'npm run build' src",
    "lint": "standard",
    "test": "ava --require babel-core/register --verbose",
    "coverage": "nyc npm test && nyc report --reporter=lcov",
    "postinstall": "npm run build",
    "prepublish": "npm run lint & npm run test",
    "precommit": "npm run lint & npm run test",
    "prepush": "npm run lint & npm run test"
  },
  "author": "Zach Eagle",
  "license": "MIT",
  "repository": "zacheagle/dockerps",
  "dependencies": {
    "chalk": "^1.1.1",
    "cli-table2": "^0.2.0",
    "lodash": "^4.3.0",
    "meow": "^3.7.0",
    "moment": "^2.11.2"
  },
  "devDependencies": {
    "ava": "^0.12.0",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.8",
    "faker": "^3.0.1",
    "husky": "^0.10.2",
    "nyc": "^5.6.0",
    "proxyquire": "^1.7.4",
    "standard": "^6.0.5",
    "watch": "^0.17.1"
  }
}
