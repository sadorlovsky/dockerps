{
  "name": "dockerps",
  "version": "0.4.1",
  "description": "Pretty-print docker ps utility",
  "main": "./lib/dockerps.js",
  "bin": {
    "dockerps": "bin/dockerps.js"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "watch 'npm run build' src",
    "lint": "eslint src test",
    "test": "NODE_ENV=test nyc ava",
    "clean": "rimraf lib",
    "check": "npm run lint && npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run clean && npm run check && npm run build",
    "precommit": "npm run check",
    "prepush": "npm run check"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Zach Orlovsky <sadorlovsky@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadorlovsky/dockerps.git"
  },
  "keywords": [
    "dockerps",
    "pretty",
    "docker",
    "ps",
    "cli",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/sadorlovsky/dockerps/issues"
  },
  "homepage": "https://github.com/sadorlovsky/dockerps",
  "dependencies": {
    "chalk": "^1.1.1",
    "cli-table2": "^0.2.0",
    "got": "^6.3.0",
    "meow": "^3.7.0",
    "query-string": "^4.2.2"
  },
  "devDependencies": {
    "@sadorlovsky/eslint-config": "^0.8.1",
    "ava": "^0.15.1",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-rewire": "^1.0.0-rc-3",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.1",
    "eslint-plugin-ava": "^2.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.1",
    "faker": "^3.0.1",
    "husky": "^0.11.4",
    "mz": "^2.4.0",
    "nock": "^8.0.0",
    "nyc": "^6.4.4",
    "rimraf": "^2.5.2",
    "watch": "^0.18.0"
  }
}
