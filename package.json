{
  "name": "dockerps",
  "version": "0.4.0",
  "description": "Pretty-print docker ps utility",
  "main": "./lib/index.js",
  "bin": {
    "dockerps": "bin/dockerps.js"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "watch 'npm run build' src",
    "lint": "standard",
    "test": "nyc ava --verbose",
    "clean": "rimraf lib",
    "check": "npm run lint && npm run test",
    "codecov": "nyc report --reporter=lcov | codecov",
    "prepublish": "npm run clean && npm run check && npm run build",
    "precommit": "npm run check",
    "prepush": "npm run check"
  },
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Zach Eagle <mazaxakep@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacheagle/dockerps.git"
  },
  "keywords": [
    "dockerps",
    "pretty",
    "docker",
    "ps",
    "cli",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/zacheagle/dockerps/issues"
  },
  "homepage": "https://github.com/zacheagle/dockerps",
  "dependencies": {
    "chalk": "^1.1.1",
    "cli-table2": "^0.2.0",
    "lodash": "^4.6.1",
    "meow": "^3.7.0",
    "moment": "^2.12.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "codecov": "^1.0.1",
    "faker": "^3.0.1",
    "husky": "^0.11.3",
    "nyc": "^6.0.0",
    "proxyquire": "^1.7.4",
    "rimraf": "^2.5.2",
    "standard": "^6.0.8",
    "watch": "^0.17.1"
  }
}
